<script>
function overwriteCssValue(selectorText, cssAttribute, newValue) {
    var styleSheets = document && document.styleSheets
        ? Array.prototype.slice.call(document.styleSheets)
        : [];

    for (var i = 0; i < styleSheets.length; i++) {
        try {
            // Ensure the stylesheet has rules we can access
            if (!styleSheets[i] || !styleSheets[i].cssRules) {
                continue;
            }

            for (var j = 0; j < styleSheets[i].cssRules.length; j++) {
                var ruleSelectorText = styleSheets[i].cssRules[j] && styleSheets[i].cssRules[j].selectorText
                    ? styleSheets[i].cssRules[j].selectorText
                    : '';

                if (ruleSelectorText && ruleSelectorText.trim() === selectorText.trim()) {
                    // Update the CSS property safely
                    styleSheets[i].cssRules[j].style.setProperty(cssAttribute, newValue);
                    return true;
                }
            }
        } catch (e) {
            console.warn('Could not access stylesheet:', e);
        }
    }
    return false; // If no matching rule was found
}
